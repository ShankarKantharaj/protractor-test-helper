{
  "name": "protractor-test-helper",
  "version": "0.0.1",
  "license": "MIT",
  "keywords": [
    "protractor",
    "e2e",
    "end-to-end",
    "test",
    "testing"
  ],
  "main": "dist",
  "types": "dist",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "pretest": "yarn build && yarn copy:lib",
    "test": "yarn test:flow && yarn test:async && yarn test:simplejs",
    "test:flow": "ng e2e --config test/protractor.flow.conf --delete-output-path=false",
    "test:async": "ng e2e --config test/protractor.async.conf --delete-output-path=false",
    "test:async:node8": "ng e2e --config test/protractor.async.node8.conf --delete-output-path=false",
    "test:simplejs": "protractor test/protractor.simplejs.conf",
    "build": "yarn clean:all && tsc -p ./tsconfig.build.json",
    "build:testapp": "ng build",
    "clean:all": "rm -rf ./dist && yarn clean:modules",
    "clean:modules": "rm -rf node_modules/protractor-test-helper",
    "copy:lib": "copyfiles package.json './dist/**/*' node_modules/protractor-test-helper",
    "pretty": "prettier --list-different --config ./.prettierrc \"**/*.@(js|ts|scss)\"",
    "pretty:write": "prettier --write --config ./.prettierrc \"**/*.@(js|ts|scss)\"",
    "doc:md": "typedoc --out ./docs/markdown ./lib --target ES5 --module commonjs --theme markdown --exclude index",
    "readme:update": "node tools/readmegenerator.js",
    "readme:validate": "yarn readme:update && git diff --exit-code",
    "webdriver:update": "webdriver-manager update"
  },
  "peerDependencies": {
    "protractor": "~5.1.2"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "copyfiles": "^2.0.0",
    "core-js": "^2.4.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "prettier": "^1.12.1",
    "protractor": "~5.1.2",
    "rxjs": "^5.5.6",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typedoc": "^0.11.1",
    "typedoc-plugin-markdown": "^1.1.9",
    "typescript": "~2.5.3",
    "zone.js": "^0.8.19"
  }
}
